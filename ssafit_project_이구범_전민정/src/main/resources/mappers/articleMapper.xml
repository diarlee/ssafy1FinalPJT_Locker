<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.ssafit.model.dao.ArticleDao">
	
	
	<select id="selectAll" resultType="Article">
		select * 
		from article;
	</select>
	
	<select id="selectOne" parameterType="int" resultType="Article">
		select *
		from article 
		where aricleId = #{articleId};
	</select>
	
	<insert id="insertArticle" parameterType="Article">
		insert into (userId, title, content, image)
		values(#{userId}, #{title}, #{content}, #{image});
	</insert>
	
	<delete id="updateArticle" parameterType="Article">
		
	</delete>
</mapper>